<h1>Login</h1>
<form action="<%=  session_index_url %>" method="post" >
  <%= render "shared/form_header" %>
  <p><label>
    Username:
    <input type="text" id="user_user_name" name="user[user_name]">
  </label>
</p>
  <p><label>
    Password:
    <input type="password" id="user_password" name="user[password]">
  </label>
</p>
  <input type="submit">
</form>